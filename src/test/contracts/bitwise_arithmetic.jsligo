/* Test JsLIGO bitwise operators */

const or_op  = (n : nat) : nat => Bitwise.or (n, 4n)
const and_op = (n : nat) : nat => Bitwise.and (n, 7n)
const xor_op = (n : nat) : nat => Bitwise.xor (n, 7n)
const lsl_op = (n : nat) : nat => Bitwise.shift_left (n, 7n)
const lsr_op = (n : nat) : nat => Bitwise.shift_right (n, 7n)
