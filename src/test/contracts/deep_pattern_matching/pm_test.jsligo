type myt =
  | ["Nil"]
  | ["Cons", [int, int]];

// this essentially test variable substitution (see var rule)
let t2 = (x: myt, y: myt): int => {
  return match(x) {
    when(Nil()):
      match(y) {
        when(Nil()): 1;
        when(Cons(p)): ((p: [int, int]) => {
          let [_a, b] = p;
          let a1 = "a";
          return (int(String.length(a1))) + b;
        })(p)
      };
    when(Cons(p)): ((p: [int, int]) => {
      let [a, b]: [int, int] = p;
      let old_b = b;
      let b_fn = (a: int, b: int): int => {
        return match(y) {
          when(Nil()): (() => {
            let f = (b: int): int => b + a;
            return f(b + 1)
          })();
          when(Cons(_)): a + b
        }
      };
      b = b_fn(a, b);
      return a + b + old_b;
    })(p)
  }
}
