type parameter = ["Increment", int] | ["Extend", never];

type storage = int;

@entry
const main = (action: parameter, store: storage): [list<operation>, storage] => {
  return [
    (list([]) as list<operation>),
    (
      match(
        action,
        {
          Increment: (n: int) => store + n,
          Extend: (k: never) => (Tezos.never(k) as storage)
        }
      )
    )
  ]
};
