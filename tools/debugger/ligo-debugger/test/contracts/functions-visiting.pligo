function f(const a: int): int is block {
    var x := a + 100;
    var x2 := x + 100
} with x2

function g(const _: unit): int is block {
    var y := 10 + 10
} with y

function main(const _ : unit; const s: int) : list(operation) * int is block {
    var s2 := s + 1;
    var s3 := g(Unit) + f(s2);
} with ((list [] : list(operation)), s3)
