type variants =
    ["Variant1", int]
  | ["Variant2"]
  | ["Variant3", option<int>];

@entry
let main = ([p, s] : [variants, int]) : [list<operation>, int] => {
  const s2 = match(p, {
    Variant1: (x : int) => s + x,
    Variant2: () => 0,
    Variant3: (p2 : option<int>) => match(p2, {
      Some: (x : int) => s + 3 * x,
      None: () => s
    })
  });

  return [(list([]) as list<operation>), s2]
};
