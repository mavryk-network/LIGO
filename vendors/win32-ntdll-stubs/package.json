{
  "name": "ligo-win32-ntdll-stubs",
  "version": "0.1.0",
  "esy": {
    "build": [
      "bash -c #{os == 'windows' ? 'make': 'true'}",
      [
        "bash",
        "-c",
        "#{os == 'windows' ? 'make install': 'true'}"
      ]
    ],
    "exportedEnv": {
      "LIGO_NTDLL_PATH": {
        "val": "#{self.lib}",
        "scope": "global"
      }
    }
  }
}
